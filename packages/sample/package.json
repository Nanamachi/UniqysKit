{
  "name": "@uniqys/sample",
  "version": "0.0.1",
  "description": "sample app of Uniqys Kit",
  "author": "Uniqys Project (https://uniqys.net)",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "prebuild": "run-p prebuild:*",
    "prebuild:config": "run-p prebuild:config:*",
    "prebuild:config:genesis": "json2ts ./config-schema/genesis.json ./src/config/genesis.d.ts",
    "prebuild:config:key": "json2ts ./config-schema/key.json ./src/config/key.d.ts",
    "build": "tsc -p tsconfig.build.json",
    "sample:easy": "ts-node src/easy.ts",
    "sample:easy-app": "ts-node src/app/easy-app.ts",
    "sample:easy-client": "ts-node src/app/easy-client.ts",
    "sample:p2p": "ts-node src/app/p2p.ts"
  },
  "dependencies": {
    "@uniqys/blockchain": "^0.0.1",
    "@uniqys/config-loader": "^0.0.1",
    "@uniqys/chain-core": "^0.0.1",
    "@uniqys/easy-client": "^0.0.1",
    "@uniqys/easy-framework": "^0.0.1",
    "@uniqys/easy-types": "^0.0.1",
    "@uniqys/merkle-patricia-trie": "^0.0.1",
    "@uniqys/p2p-network": "^0.0.1",
    "@uniqys/serialize": "^0.0.1",
    "@uniqys/signature": "^0.0.1",
    "@uniqys/store": "^0.0.1",
    "axios": "^0.18.0",
    "debug": "^3.1.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "memcached": "^2.2.2",
    "memdown": "^2.0.0",
    "meow": "^5.0.0",
    "peer-info": "^0.14.1"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-router": "^7.0.30",
    "@types/memcached": "^2.2.5",
    "@types/meow": "^4.0.1",
    "@types/node": "^10.5.2",
    "@uniqys/type-definitions": "^0.0.1",
    "json-schema-to-typescript": "^5.5.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
